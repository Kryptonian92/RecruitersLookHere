var1 = ("This is the first function")
var2 = ("This is the second function")
var3 = ("This is the third function")

def function1():
    print (var1)

def function2():
    print (var2)

def function3():
    print (var3)

def main():
    print ("This is the main function")
    function1()
    function2()
    function3()

main()
